        {
            "name": "Simulation",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test_simulation.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Math model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test_math_model.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal",
        }
    ]
